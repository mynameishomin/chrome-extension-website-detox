{
    "compilerOptions": {
        "target": "es2017",
        "lib": ["dom", "es2017"],
        "outDir": "./dist",
        "rootDir": "./src",
        "moduleResolution": "node",
        "module": "es6",
        "types": ["chrome"],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
}
